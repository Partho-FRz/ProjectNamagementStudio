
<!DOCTYPE html>
<html>
<head>

    <title>Account</title>

    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link href="~/Bootstrap/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Bootstrap/style.css" rel="stylesheet" />
    <link href="~/Bootstrap/bootstrap.min.js" rel="stylesheet" />
    <link href="~/Bootstrap/bootstrap.bundle.js" rel="stylesheet" />
    <link href="~/Bootstrap/bootstrap.js" rel="stylesheet" />


</head>

<body>
    @if (ViewBag.msg != null)
    {
        <script type="text/javascript">
            window.onload = function () {
                alert("@ViewBag.msg");
            };


        </script>
    }
    <nav class="navbar navbar-expand-lg navbar-dark bg-dark">

        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
        </button>

        <div class="collapse navbar-collapse" id="navbarSupportedContent">
            <ul class="navbar-nav mr-auto">
                <li class="nav-item">
                    <a class="nav-link" href="http://localhost:52320/Ceoacc/CeoCon"><</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="http://localhost:52320/Homepage">Home</a>
                </li>

                <li class="nav-item">
                    <a class="nav-link" href="http://localhost:52320/News">News</a>
                </li>

                <li class="nav-item active">
                    <a class="nav-link" href="http://localhost:52320/@Session["UserAcc"]">Account<span class="sr-only">(current)</span></a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="http://localhost:52320/@Session["LogoutAcc"]">Log Out</a>
                </li>

            </ul>

        </div>
    </nav>


    <table width="100%" cellspacing="0" cellpadding="10" border="1">
        <tr>
            <td colspan="2" valign="middle" height="70">
                <table width="100%">
                    <tr>
                        <td>
                            <a href="account/dashboard.html" target="contentFrame">
                                <img height="48" src="~/Picture/visual_studio_purple.png">
                            </a>
                        </td>

                    </tr>
                </table>
            </td>
        </tr>
        <tr>
            <td width="220" valign="top">
                <ul>
                    <li><a href="/Project/CreateProject">Create project</a></li>
                    <li><a href="/Project/ViewProjects">View projects</a></li>
                    <li><a href="/Project/CloseProject">Close project</a></li>
                </ul>




            </td>
            <td valign="top">
                <table class="table">
                    <h1> Closed Projects</h1>
                    <tr>
                        <th>
                            Name
                        </th>
                        <th>
                            Budget
                        </th>
                        <th>
                            Starting Date
                        </th>
                        <th>
                            Ending Date
                        </th>
                    </tr>

                   
                </table>
                <table class="table">
                    <h1>Running Projects</h1>
                    <tr>
                        <th>
                            Name
                        </th>
                        <th>
                            Budget
                        </th>
                        <th>
                            Starting Date
                        </th>

                    </tr>
                    
                </table>

            </td>
        </tr>
    </table>
</body>
</html>
